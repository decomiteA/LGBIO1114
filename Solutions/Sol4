# Passe-bas 

b,a = CreateFilter(6,'low',0.5)
lowpasssig = signal.filtfilt(b,a,input_signal)
b1,a1 = CreateFilter(6,'high',2500/10000)
higpasssig = signal.filtfilt(b1,a1,input_signal)
b2,a2 = CreateFilter(6,'bandpass',[0.2,0.8])


Tf1,f1 = computeTF(t_vector,lowpasssig)
tf_sig = abs(Tf1[:len(input_signal)//2]/len(input_signal))
RepresFreq(f1,tf_sig)
